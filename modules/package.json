{
  "name": "RenderTheFuck",
  "version": "0.1.1",
  "description": "A MediaWiki extension.",
  "author": "Raymond Wu",
  "private": true,
  "babel": {
    "presets": [
      ["env", {
        "targets": {
          "browsers": ["ie >= 9"]
        }
      }]
    ]
  },
  "scripts": {
    "build": "yarn lint && yarn browserify && yarn uglify && yarn clean",
    "lint": "eslint --ext es6 .",
    "browserify": "browserify render.es6 -o render.js -t babelify",
    "uglify": "uglifyjs render.js -c -o render.min.js",
    "clean": "rm -f render.js",
    "cssmin": "cssmin timeline.css > timeline.min.css",
    "lint-json": "jsonlint examples/stack.tfj -q -c"
  },
  "devDependencies": {
    "babelify": "7.3.0",
    "browserify": "13.1.1",
    "cssmin": "0.4.3",
    "d3": "4.4.0",
    "eslint": "3.6.1",
    "jsonfile": "2.4.0",
    "jsonlint": "1.6.0",
    "jsonschema": "1.1.1",
    "uglify-js": "2.7.5",
    "babel-preset-env": "1.6.1"
  }
}
